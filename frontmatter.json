{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "linktitle",
          "name": "linktitle",
          "type": "string"
        },
        {
          "title": "menu",
          "name": "menu",
          "type": "fields",
          "fields": [
            {
              "title": "main",
              "name": "main",
              "type": "fields",
              "fields": [
                {
                  "title": "parent",
                  "name": "parent",
                  "type": "string"
                }
              ]
            }
          ]
        },
        {
          "title": "prev",
          "name": "prev",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "weight",
          "name": "weight",
          "type": "number"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    }
  ],
  "frontMatter.git.enabled": true
}